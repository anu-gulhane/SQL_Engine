LOAD Student
LOAD Marks
LOAD Advisor
CROSS_STUDENT_ADVISOR <- CROSS Student Advisor
EXPORT CROSS_STUDENT_ADVISOR

JOIN_STUDENT_ADVISOR <- SELECT Advisor_id == Adv_Id FROM CROSS_STUDENT_ADVISOR

EXPORT JOIN_STUDENT_ADVISOR
QUESG <- SELECT Adv_age > 40 FROM JOIN_STUDENT_ADVISOR
EXPORT QUESG

1G <- PROJECT Stud_Id,Advisor_id FROM QUESG
EXPORT 1G
