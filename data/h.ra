LOAD Student
LOAD Marks
LOAD Advisor
CROSS_STUDENT_MARKS <- CROSS Student Marks
EXPORT CROSS_STUDENT_MARKS

JOIN_STUDENT_MARKS <- SELECT Student_Stud_Id == Marks_Stud_Id FROM CROSS_STUDENT_MARKS 
EXPORT JOIN_STUDENT_MARKS
TEMP1 <- SELECT Advisor_id == 2 FROM JOIN_STUDENT_MARKS
EXPORT TEMP1
QUESH <- SELECT Maths_marks > 90 FROM TEMP1
EXPORT QUESH

1H <- PROJECT Student_Stud_Id FROM QUESH
EXPORT 1H 
